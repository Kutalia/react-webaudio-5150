@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://faustlv2.bitbucket.io/kpp_tubeamp>
    a lv2:Plugin ;
    lv2:binary <kpp_tubeamp.so> ;
    rdfs:seeAlso <kpp_tubeamp.ttl> .
    
<https://faustlv2.bitbucket.io/kpp_tubeampui>
    a ui:X11UI ;
    ui:binary <kpp_tubeampui.so> ;
        lv2:requiredFeature ui:idleInterface ; 
        lv2:requiredFeature ui:fixedSize;
        lv2:extensionData ui:idleInterface ; 
        ui:requiredFeature ui:makeResident .
    
<https://faustlv2.bitbucket.io/kpp_tubeamp> a lv2:FilterPlugin .

